install.packages("trafo")
library(trafo)
vr=read.csv(file="/Users/xxxibqxy/Downloads/dissertation/heatmap.csv",header=TRUE,sep=",",quote=NULL)
df<-data.frame(vr)
trafo_lm()
linMod<-lm(Migration_flow_1990~.,data=df)
assumptions(linMod)
linMod_trafo <- trafo_lm(linMod)
diagnostics(linMod_trafo)
plot(linMod_trafo)
linMod_trafo2 <- trafo_lm(object = linMod, trafo = "logshiftopt",
                          method = "skew")
boxcox_1990 <- boxcox(linMod)
log_1990 <- logtrafo(linMod)
head(as.data.frame(boxcox_1990))
linMod_comp <- trafo_compare(object = linMod,
                             trafos = list(boxcox_1990, log_1990))
diagnostics(linMod_comp)
df1<-data.frame(boxcox_1990)
